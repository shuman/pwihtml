@import 'jeet'

html, body
  position: fixed
  -webkit-overflow-scrolling: touch
  width: 100%
  height: 100%
  min-width: 100%
  min-height: 100%

.wrapper
  min-height: 100%
  height: 100%
  overflow: hidden
  center(1920px)
  canvas
    margin: auto
    center(1920px)
    background-image: url('../images/rsz_bg.png')
    background-repeat: no-repeat
    background-size: cover
